{
  "name": "rollup-terser",
  "version": "1.0.1",
  "type": "commonjs",
  "description": "Rollup plugin to minify generated bundle",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "default": "./dist/index.js"
  },
  "files": [
    "dist",
    "!dist/**/*.map",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "jest"
  },
  "dependencies": {
    "terser": "^5.15.1"
  },
  "peerDependencies": {
    "rollup": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    }
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "terser",
    "minify"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wutility/rollup-terser.git"
  },
  "author": "Haikel Fazzani",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@types/node": "^18.8.5",
    "jest": "^29.1.2",
    "rollup": "^3.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}